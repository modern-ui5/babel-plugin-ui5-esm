    "require"
  ],
  "author": "Yichuan Shen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/modern-ui5/babel-plugin-ui5-esm/issues"
  },
  "homepage": "https://github.com/modern-ui5/babel-plugin-ui5-esm#readme",
  "peerDependencies": {
    "@babel/core": "7.x"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@types/babel__core": "^7.20.1",
    "@types/mocha": "^10.0.1",
    "babel-plugin-tester": "^11.0.4",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6"
  }
