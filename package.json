  "description": "A Babel plugin that transforms ES module-style UI5 imports into UI5-style requires",
  "main": "./dist/main.js",
  "type": "module",
  "scripts": {
    "build": "tsc --project ./tsconfig.build.json",
    "test": "tsc --project ./tsconfig.test.json && mocha \"./test_dist/**/*.test.js\""
